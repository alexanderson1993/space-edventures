    "validate": "npm run test && npm run prettier -- --list-different",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|mdx|graphql|vue)\"",
    "precommit": "lint-staged",
    "postinstall": "npm install --prefix functions",
    "schema:check": "npx apollo service:check --endpoint=http://localhost:5000/space-edventures-beta/us-central1/api/graphql",
